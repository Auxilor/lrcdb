generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Config {
  id        String @id @default(uuid()) @map("_id")
  name      String
  plugin    String
  contents  String
  downloads Int    @default(0)
  views     Int    @default(0)
}
